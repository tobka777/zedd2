{
  "name": "zedd-app",
  "productName": "zedd-app",
  "version": "2.12.0",
  "description": "Zedd-App Time Tracker",
  "main": ".webpack/main",
  "repository": "github:tobka777/zedd2",
  "scripts": {
    "start": "cross-env NODE_INSTALLER=npm electron-forge start -- --dev",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "genicons": "rimraf icons && ts-node -P genIcons.tsconfig.json genIcons.tsx",
    "prettier": "prettier --write src/**/*.{ts,tsx,js,d.ts,json}",
    "test": "cross-env TS_NODE_TRANSPILE_ONLY=1 TS_NODE_COMPILER_OPTIONS={\\\"module\\\":\\\"commonjs\\\"} mocha --require ts-node/register src/**/Undoer.test.ts",
    "test:watch": "npm t -- --extension ts,tsx --watch",
    "tslint": "tslint --project .",
    "lint": "eslint src --ext .ts,.js,.tsx,.jsx"
  },
  "keywords": [],
  "author": {
    "name": "Andrej Bespalov",
    "email": "andrej.bespalov@capgemini.com"
  },
  "license": "UNLICENSED",
  "prettier": {
    "trailingComma": "all",
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "printWidth": 100
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "ignore": [
          "\\.gitignore",
          "node_modules/\\.cache",
          ".*\\.(iobj|pdb|ipdb)$"
        ],
        "derefSymlinks": true,
        "icon": "icons/app"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "zedd",
            "remoteReleases": "https://github.com/tobka777/zedd2/releases/latest/download"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        }
      ],
      "plugins": [
        {
          "name": "@electron-forge/plugin-webpack",
          "config": {
            "mainConfig": "./webpack.main.config.js",
            "devContentSecurityPolicy": "connect-src 'self' * 'unsafe-inline' blob: data: gap:",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.ts",
                  "name": "main_window"
                }
              ]
            }
          }
        }
      ],
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "tobka777",
              "name": "zedd2"
            },
            "prerelease": false
          }
        }
      ]
    }
  },
  "dependencies": {
    "selenium-webdriver": "^4.1.1",
    "win-ca": "^3.4.5"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@electron-forge/cli": "^6.0.5",
    "@electron-forge/maker-deb": "^6.0.5",
    "@electron-forge/maker-rpm": "^6.0.5",
    "@electron-forge/maker-squirrel": "^6.0.5",
    "@electron-forge/maker-zip": "^6.0.5",
    "@electron-forge/plugin-webpack": "^6.0.5",
    "@electron-forge/publisher-github": "^6.0.5",
    "@electron/remote": "^2.0.4",
    "@emotion/css": "^11.10.5",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@fiahfy/icns-convert": "^0.0.12",
    "@fiahfy/ico-convert": "^0.0.12",
    "@mui/icons-material": "^5.11.9",
    "@mui/material": "^5.11.9",
    "@types/lodash": "^4.14.178",
    "@types/mocha": "^9.1.0",
    "@types/node": "^18.6.1",
    "@types/node-windows": "^0.1.2",
    "@types/react": "^18.0.15",
    "@types/react-date-range": "^1.4.3",
    "@types/react-dom": "^18.0.6",
    "@types/selenium-webdriver": "^4.0.18",
    "@types/sinon": "^10.0.11",
    "@types/unzipper": "^0.10.5",
    "@types/webpack-env": "^1.16.3",
    "@types/webpack-node-externals": "^2.5.3",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "@vercel/webpack-asset-relocator-loader": "^1.7.0",
    "async-which": "^1.0.3",
    "babel-loader": "^8.2.3",
    "babel-plugin-import": "^1.13.3",
    "browser-request": "^0.3.3",
    "chroma.ts": "^1.0.10",
    "convert-svg-to-png": "^0.6.4",
    "cross-env": "^7.0.3",
    "css-loader": "^6.6.0",
    "date-fns": "^2.28.0",
    "electron": "^23.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "fast-csv": "^4.3.6",
    "jira.js": "^2.10.4",
    "lodash": "^4.17.21",
    "mobx": "^6.4.0",
    "mobx-react-lite": "^3.3.0",
    "mobx-utils": "^6.0.4",
    "mocha": "^10.0.0",
    "natural-orderby": "^2.0.3",
    "node-loader": "^2.0.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "raw-loader": "^4.0.2",
    "react": "^18.2.0",
    "react-date-range": "^1.4.0",
    "react-dom": "^18.2.0",
    "react-hotkeys": "^2.0.0",
    "react-markdown": "^8.0.0",
    "request": "^2.88.2",
    "serializr": "^2.0.5",
    "sharp": "^0.30.7",
    "sinon": "^14.0.0",
    "sleep-promise": "^9.1.0",
    "style-loader": "^3.3.1",
    "sudo-prompt": "^9.2.1",
    "tmp-promise": "^3.0.3",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.5.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.5.5",
    "unzipper": "^0.10.11",
    "zedd-clarity": "file:../zedd-clarity"
  }
}
